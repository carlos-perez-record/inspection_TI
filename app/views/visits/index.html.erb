<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">

      <div class="visits">
        <% @visits.each do |visit| %>
        <div class="visit clearfix">
          <div class="visit-details">
            <span class="visit-title"><%= link_to visit.id, visit %></span>
            <span class="visit-title"><%= visit.fecha %></span>
            <span class="visit-description"><%= link_to "Editar", edit_visit_path(visit) %></span>
            <span class="visit-description"><%= link_to "Eliminar", visit, method: :delete, data: { confirm: "¿Estás seguro de eliminar esta visita?" } %></span>
          </div>
        </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
